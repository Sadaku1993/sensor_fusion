<?xml version="1.0"?>
<launch>
    <!--Launch Sensor Node-->
	<!--include file="$(find sq1_extra)/launch/run_sq2_for_joy.launch"/-->

    <!--Remove Ground-->
    <!--node pkg="sensor_fusion" type="sq_rm_ground" name="sq_rm_ground">
		<remap from="/cloud" 	 to="/lcl/center" />
		<remap from="/rm_ground" to="/rm_ground/center" />
    </node-->

	<!--Plane Segmentation-->
	<node pkg="sensor_fusion" type="lidar_seg_plane" name="lidar_seg_plane">
		<remap from="/cloud"  to="/rm_ground/center" />
		<remap from="/plane"  to="/center/plane" />
	</node>

    <!--Circle Detection-->
    <node pkg="sensor_fusion" type="lidar_seg_circle" name="lidar_seg_circle" output="screen">
        <remap from="/cloud"  to="/center/plane" />
        <remap from="/output" to="/dumy/centroid" />
    </node>

	<!--Dumy PointCloud-->
	<node pkg="sensor_fusion" type="dumy_lidar" name="duny_lidar">
		<remap from="/cloud" to="/center/plane" />
		<remap from="/output" to="/dumy" />
	</node>

	<!--PointCloud2Image-->
	<node pkg="sensor_fusion" type="pointcloud2image" name="pointcloud2image" >
		<remap from="/cloud" to="/center/plane" />
	</node>
	
	<!--divide PointCloud-->
	<!--node pkg="sensor_fusion" type="detection_circle" name="detection_circle" >
		<remap from="/image" 	to="/zed0/left/image_rect_color/republish" />
	</node-->
	
</launch>
