<?xml version="1.0"?>
<launch>
    <!--Launch Zed Wrapper-->
	<include file="$(find zed_wrapper)/launch/zed0.launch"/>

    <!--Down sampling-->
    <param name="camera/ds_size" type="double" value="0.03" />
    <node pkg="sensor_fusion" type="camera_downsample" name="camera_downsample">
        <remap from="/cloud" to="/zed0/point_cloud/cloud_registered" />
        <remap from="/output" to="/zed0/ds_cloud" />
    </node>
</launch>
